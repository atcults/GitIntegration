{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/src/GitIntegration/bin/Debug/net8.0/GitIntegration.dll",
            "args": [],
            "cwd": "${workspaceRoot}/src/GitIntegration/bin/Debug/net8.0",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
